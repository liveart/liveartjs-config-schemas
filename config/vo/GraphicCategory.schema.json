  "title": "GraphicCategory",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "thumb": {
      "type": "string"
    },
    "graphicsList": {
      "type": "array",
